<div class="container-fluid">
  <div class="row-fluid">
    <div class="span2">
      <div class="well sidebar-nav">
        <ul class="nav nav-list">
          <li>
            <form id="searchform" class="form-horizontal">
              <div class="input-prepend input-append">
                <span class="add-on"><i class="icon-search"></i></span>
                <input class="span10" type="text" placeholder="Search" id="searchquery" name="searchdom">
              </div>
            </form>
          </li>
          <li class="">
            <a class=""  data-toggle="ajaxmodal" title="" name="newalias" href="/add"><i class="icon-plus"></i>
              Add Domain
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="span10">
    <!--Body content-->  
        <table class="class">
          <thead>
            <tr>
              <th>Name</th>
              <th>Master</th>
              <th>last_check</th>
              <th>type</th>
              <th>notified_serial</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <% @domains.each do |domain| %>
            <tr>
              <td><a href='/list/<%= domain[:name]%>/'><%= domain[:name]%></a></td>
              <td><%= domain[:master]%></td>
              <td><%= domain[:last_check]%></td>
              <td><%= domain[:type]%></td>
              <td><%= domain[:notified_serial]%></td>
              <td><a href="/delete/dommain/<%= domain[:id] %>" class="btn btn btn-mini"><i class="icon-remove-circle"></i></a></td>
            </tr>
            <% end %>
          <tbody>
      </table>
    </div>
  </div>
</div>


